{
  "name": "next-msw-integration",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:e2e": "NEXT_PUBLIC_ENABLE_MSW_MOCK=true APP_ENV=test pnpm build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "pnpm build:e2e && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install",
    "clean": "rm -rf pnpm-lock.yaml .next node_modules playwright-report test-results .playwright-mcp",
    "prepare": "husky",
    "prettier": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.10",
    "msw": "^2.12.4",
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.0.10",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-config-ts-prefixer": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}
