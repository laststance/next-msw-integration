{
  "name": "next-msw-integration",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:e2e": "NEXT_PUBLIC_ENABLE_MSW_MOCK=true APP_ENV=test pnpm build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "pnpm build:e2e && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install",
    "clean": "rm -rf pnpm-lock.yaml .next node_modules playwright-report test-results .playwright-mcp",
    "prepare": "husky",
    "prettier": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.8",
    "msw": "^2.12.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@jest/globals": "^30.2.0",
    "@next/eslint-plugin-next": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "eslint-config-ts-prefixer": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}
